
(defvar my-json "[1, 2, 3, 4]")

(defwidget keybinds []
  (box :class "metric"
    (label :text "How are you?")
    (box
      (for entry in my-json
        (button :onclick "notify-send 'click' 'button ${entry}'"
          entry)))
  )
)

(defwindow keybindsWindow
  :monitor 0
  :geometry (geometry :x "50%"
    :y "50%"
    :width "30%"
    :height "30%"
  :anchor "center")
  :stacking "fg"
  :focusable false
  (keybinds))